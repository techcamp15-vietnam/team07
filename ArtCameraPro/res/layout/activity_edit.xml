            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/btSelectColor"
            android:layout_toRightOf="@id/btSelectColor"
            android:background="@drawable/selector_constrast_toggle" />

        <ImageButton
