            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/btSelectColor"
            android:layout_toRightOf="@id/btSelectEffect"
            android:background="@drawable/selector_troll_toggle" />

        <ImageButton
            android:id="@+id/btShare"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/btSelectColor"
            android:layout_alignParentRight="true"
            android:background="@drawable/selector_social_upload" />
    </RelativeLayout>

